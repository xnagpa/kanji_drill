.nav
    background-color: white
    width: 100%
    text-align: left
    font-family: Helvetica, Arial, sans-serif
    font-size: 12px
    box-shadow: 0 2px 4px #000000
    padding-top: 43px
    padding-bottom: 43px

.nav ul
    margin-left: 130px

.nav ul li
    display: inline
    margin-right: 40px
    color: #8091A5
    
    padding-bottom: 37px
    &:hover { border-bottom: 6px solid #0099FA }

td
    border: 1px solid #DFE6EE
    padding: 20px 36px 20px 36px

.styled_select
    background: url(http://i62.tinypic.com/15xvbd5.png) no-repeat 96% 0;
    width: 280px
    height: 40px
    overflow: hidden

.styled_select select
    border: 1px solid #B0BAC5
    border-radius: 4px
    padding: 12px
    
  


